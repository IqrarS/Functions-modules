    "def reverse(str):\n",
    "    s = \"\"\n",
    "    for ch in str:\n",
    "        s = ch + s\n",
    "    return s\n",
    "samplestring = \"1234abcd\"\n",
    "print(\"given string:\",samplestring)\n",
    "print(\"reversed string:\",reverse(samplestring))"
   ]
